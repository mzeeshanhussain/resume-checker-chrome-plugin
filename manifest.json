{
    "manifest_version": 3,
    "name": "Resume Checker",
    "version": "1.0",
    "description": "Analyze your resume with job descriptions.",
    "permissions": ["storage", "scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}
